geom_line() +
geom_point() +
geom_text(aes(label=floor(Timestamp/1000)), hjust=1, vjust=1) +
facet_grid(Station ~ .) +
ggtitle(paste0("ID=", IDPar,
", AnalysisResult=", dataProduct$AnalysisResult[1],
", MNo=", dataProduct$MaterialNo[1])) +
scale_x_continuous(limits = c(0, 30000))
